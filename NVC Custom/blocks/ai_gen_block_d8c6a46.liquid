{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-luxury-eyewear-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.section_padding }}px 0;
  }

  .ai-luxury-eyewear-container-{{ ai_gen_id }} {
    max-width: {{ block.settings.container_width }}px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-luxury-eyewear-hero-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 60px;
  }

  .ai-luxury-eyewear-headline-{{ ai_gen_id }} {
    font-size: {{ block.settings.headline_size }}px;
    font-weight: 600;
    color: {{ block.settings.text_color }};
    margin-bottom: 20px;
    line-height: 1.2;
  }

  .ai-luxury-eyewear-subheadline-{{ ai_gen_id }} {
    font-size: {{ block.settings.subheadline_size }}px;
    color: {{ block.settings.text_color }};
    line-height: 1.6;
    max-width: 800px;
    margin: 0 auto 40px;
  }

  .ai-luxury-eyewear-shop-selector-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 20px;
    margin-bottom: 40px;
  }

  .ai-luxury-eyewear-shop-option-{{ ai_gen_id }} {
    display: inline-block;
    padding: 12px 30px;
    background-color: transparent;
    color: {{ block.settings.text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_radius }}px;
    font-size: 16px;
    font-weight: 600;
    transition: all 0.3s ease;
    border: 2px solid {{ block.settings.text_color }};
    cursor: pointer;
  }

  .ai-luxury-eyewear-shop-option-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.text_color }};
    color: {{ block.settings.background_color }};
  }

  .ai-luxury-eyewear-price-banner-{{ ai_gen_id }} {
    background-color: {{ block.settings.price_banner_bg }};
    color: {{ block.settings.price_banner_text }};
    padding: 40px;
    border-radius: {{ block.settings.border_radius }}px;
    text-align: center;
    margin-bottom: 60px;
  }

  .ai-luxury-eyewear-price-{{ ai_gen_id }} {
    font-size: {{ block.settings.price_size }}px;
    font-weight: 700;
    margin-bottom: 20px;
  }

  .ai-luxury-eyewear-price-description-{{ ai_gen_id }} {
    font-size: {{ block.settings.body_text_size }}px;
    line-height: 1.6;
    margin-bottom: 30px;
  }

  .ai-luxury-eyewear-features-list-{{ ai_gen_id }} {
    list-style: none;
    padding: 0;
    margin: 0;
    text-align: left;
    max-width: 600px;
    margin: 0 auto;
  }

  .ai-luxury-eyewear-features-list-{{ ai_gen_id }} li {
    font-size: {{ block.settings.body_text_size }}px;
    padding: 12px 0;
    border-bottom: 1px solid {{ block.settings.divider_color }};
    display: flex;
    align-items: flex-start;
  }

  .ai-luxury-eyewear-features-list-{{ ai_gen_id }} li:last-child {
    border-bottom: none;
  }

  .ai-luxury-eyewear-features-list-{{ ai_gen_id }} li::before {
    content: "✓";
    color: {{ block.settings.accent_color }};
    font-weight: 700;
    margin-right: 12px;
    font-size: 18px;
  }

  .ai-luxury-eyewear-quality-section-{{ ai_gen_id }} {
    margin-bottom: 60px;
  }

  .ai-luxury-eyewear-section-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.section_title_size }}px;
    font-weight: 600;
    color: {{ block.settings.text_color }};
    text-align: center;
    margin-bottom: 40px;
  }

  .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} {
    width: 100%;
    border-collapse: collapse;
    margin-bottom: 40px;
    background-color: {{ block.settings.table_bg }};
    border-radius: {{ block.settings.border_radius }}px;
    overflow: hidden;
  }

  .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} th,
  .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} td {
    padding: 20px;
    text-align: left;
    border-bottom: 1px solid {{ block.settings.divider_color }};
  }

  .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} th {
    background-color: {{ block.settings.table_header_bg }};
    color: {{ block.settings.table_header_text }};
    font-weight: 600;
    font-size: {{ block.settings.body_text_size }}px;
  }

  .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} td {
    font-size: {{ block.settings.body_text_size }}px;
    color: {{ block.settings.text_color }};
  }

  .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} tr:last-child td {
    border-bottom: none;
  }

  .ai-luxury-eyewear-how-section-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 60px;
  }

  .ai-luxury-eyewear-how-content-{{ ai_gen_id }} {
    font-size: {{ block.settings.body_text_size }}px;
    color: {{ block.settings.text_color }};
    line-height: 1.6;
    max-width: 800px;
    margin: 0 auto;
  }

  .ai-luxury-eyewear-cta-{{ ai_gen_id }} {
    text-align: center;
    margin-top: 40px;
    display: flex;
    gap: 20px;
    justify-content: center;
    flex-wrap: wrap;
  }

  .ai-luxury-eyewear-button-{{ ai_gen_id }} {
    display: inline-block;
    padding: {{ block.settings.button_padding_v }}px {{ block.settings.button_padding_h }}px;
    background-color: {{ block.settings.button_bg }};
    color: {{ block.settings.button_text }};
    text-decoration: none;
    border-radius: {{ block.settings.button_radius }}px;
    font-size: {{ block.settings.button_text_size }}px;
    font-weight: 600;
    transition: all 0.3s ease;
    border: 2px solid {{ block.settings.button_bg }};
  }

  .ai-luxury-eyewear-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_bg }};
    color: {{ block.settings.button_hover_text }};
    border-color: {{ block.settings.button_hover_bg }};
  }

  .ai-luxury-eyewear-button-secondary-{{ ai_gen_id }} {
    display: inline-block;
    padding: {{ block.settings.button_padding_v }}px {{ block.settings.button_padding_h }}px;
    background-color: transparent;
    color: {{ block.settings.button_bg }};
    text-decoration: none;
    border-radius: {{ block.settings.button_radius }}px;
    font-size: {{ block.settings.button_text_size }}px;
    font-weight: 600;
    transition: all 0.3s ease;
    border: 2px solid {{ block.settings.button_bg }};
  }

  .ai-luxury-eyewear-button-secondary-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_bg }};
    color: {{ block.settings.button_text }};
  }

  @media screen and (max-width: 768px) {
    .ai-luxury-eyewear-headline-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.headline_size }}px * 0.7);
    }

    .ai-luxury-eyewear-subheadline-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.subheadline_size }}px * 0.9);
    }

    .ai-luxury-eyewear-price-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.price_size }}px * 0.8);
    }

    .ai-luxury-eyewear-section-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.section_title_size }}px * 0.8);
    }

    .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} th,
    .ai-luxury-eyewear-comparison-table-{{ ai_gen_id }} td {
      padding: 15px 10px;
      font-size: calc({{ block.settings.body_text_size }}px * 0.9);
    }

    .ai-luxury-eyewear-cta-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: center;
    }

    .ai-luxury-eyewear-shop-selector-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: center;
    }

    .ai-luxury-eyewear-shop-option-{{ ai_gen_id }} {
      width: 100%;
      max-width: 300px;
    }

    .ai-luxury-eyewear-button-{{ ai_gen_id }},
    .ai-luxury-eyewear-button-secondary-{{ ai_gen_id }} {
      width: 100%;
      max-width: 300px;
    }
  }
{% endstyle %}

<div class="ai-luxury-eyewear-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-luxury-eyewear-container-{{ ai_gen_id }}">
    
    <div class="ai-luxury-eyewear-hero-{{ ai_gen_id }}">
      <h1 class="ai-luxury-eyewear-headline-{{ ai_gen_id }}">
        {{ block.settings.headline }}
      </h1>
      <div class="ai-luxury-eyewear-subheadline-{{ ai_gen_id }}">
        {{ block.settings.subheadline }}
      </div>

      {% if block.settings.show_shop_selector %}
        <div class="ai-luxury-eyewear-shop-selector-{{ ai_gen_id }}">
          {% if block.settings.optical_collection != blank %}
            <a href="{{ block.settings.optical_collection.url }}" class="ai-luxury-eyewear-shop-option-{{ ai_gen_id }}">
              {{ block.settings.optical_selector_text }}
            </a>
          {% endif %}
          
          {% if block.settings.sunglasses_collection != blank %}
            <a href="{{ block.settings.sunglasses_collection.url }}" class="ai-luxury-eyewear-shop-option-{{ ai_gen_id }}">
              {{ block.settings.sunglasses_selector_text }}
            </a>
          {% endif %}
        </div>
      {% endif %}
    </div>

    <div class="ai-luxury-eyewear-price-banner-{{ ai_gen_id }}">
      <div class="ai-luxury-eyewear-price-{{ ai_gen_id }}">
        {{ block.settings.price_text }}
      </div>
      <div class="ai-luxury-eyewear-price-description-{{ ai_gen_id }}">
        {{ block.settings.price_description }}
      </div>
      
      {% if block.settings.show_features %}
        <ul class="ai-luxury-eyewear-features-list-{{ ai_gen_id }}">
          {% if block.settings.feature_1 != blank %}
            <li>{{ block.settings.feature_1 }}</li>
          {% endif %}
          {% if block.settings.feature_2 != blank %}
            <li>{{ block.settings.feature_2 }}</li>
          {% endif %}
          {% if block.settings.feature_3 != blank %}
            <li>{{ block.settings.feature_3 }}</li>
          {% endif %}
        </ul>
      {% endif %}
    </div>

    <div class="ai-luxury-eyewear-quality-section-{{ ai_gen_id }}">
      <h2 class="ai-luxury-eyewear-section-title-{{ ai_gen_id }}">
        {{ block.settings.quality_title }}
      </h2>
      
      {% if block.settings.show_comparison_table %}
        <table class="ai-luxury-eyewear-comparison-table-{{ ai_gen_id }}">
          <thead>
            <tr>
              <th>{{ block.settings.table_header_1 }}</th>
              <th>{{ block.settings.table_header_2 }}</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ block.settings.table_row_1_col_1 }}</td>
              <td>{{ block.settings.table_row_1_col_2 }}</td>
            </tr>
            <tr>
              <td>{{ block.settings.table_row_2_col_1 }}</td>
              <td>{{ block.settings.table_row_2_col_2 }}</td>
            </tr>
            <tr>
              <td>{{ block.settings.table_row_3_col_1 }}</td>
              <td>{{ block.settings.table_row_3_col_2 }}</td>
            </tr>
          </tbody>
        </table>
      {% endif %}
    </div>

    <div class="ai-luxury-eyewear-how-section-{{ ai_gen_id }}">
      <h2 class="ai-luxury-eyewear-section-title-{{ ai_gen_id }}">
        {{ block.settings.how_title }}
      </h2>
      <div class="ai-luxury-eyewear-how-content-{{ ai_gen_id }}">
        {{ block.settings.how_content }}
      </div>
    </div>

    {% if block.settings.show_cta %}
      <div class="ai-luxury-eyewear-cta-{{ ai_gen_id }}">
        {% if block.settings.optical_collection != blank %}
          <a href="{{ block.settings.optical_collection.url }}" class="ai-luxury-eyewear-button-{{ ai_gen_id }}">
            {{ block.settings.optical_text }}
          </a>
        {% endif %}
        
        {% if block.settings.sunglasses_collection != blank %}
          <a href="{{ block.settings.sunglasses_collection.url }}" class="ai-luxury-eyewear-button-secondary-{{ ai_gen_id }}">
            {{ block.settings.sunglasses_text }}
          </a>
        {% endif %}
      </div>
    {% endif %}

  </div>
</div>

