{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-legacy-story-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    padding: {{ block.settings.section_padding }}px 20px;
    position: relative;
    overflow: hidden;
  }

  .ai-legacy-story__container-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    position: relative;
    z-index: 2;
  }

  .ai-legacy-story__content-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr;
    gap: {{ block.settings.content_gap }}px;
    align-items: center;
  }

  {% if block.settings.layout == 'side-by-side' %}
    @media screen and (min-width: 992px) {
      .ai-legacy-story__content-{{ ai_gen_id }} {
        grid-template-columns: {% if block.settings.image_position == 'left' %}1fr 1fr{% else %}1fr 1fr{% endif %};
      }

      {% if block.settings.image_position == 'right' %}
        .ai-legacy-story__image-wrapper-{{ ai_gen_id }} {
          order: 2;
        }
        .ai-legacy-story__text-wrapper-{{ ai_gen_id }} {
          order: 1;
        }
      {% endif %}
    }
  {% endif %}

  .ai-legacy-story__image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    {% if block.settings.layout == 'full-width' %}
      margin-bottom: {{ block.settings.content_gap }}px;
    {% endif %}
  }

  .ai-legacy-story__image-container-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: {{ block.settings.image_aspect_ratio }}%;
    overflow: hidden;
    border-radius: {{ block.settings.image_border_radius }}px;
  }

  .ai-legacy-story__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-legacy-story__image-placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-legacy-story__image-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    max-width: 200px;
    max-height: 200px;
    opacity: 0.3;
  }

  .ai-legacy-story__text-wrapper-{{ ai_gen_id }} {
    padding: {{ block.settings.text_padding }}px;
    text-align: {{ block.settings.text_align }};
  }

  .ai-legacy-story__eyebrow-{{ ai_gen_id }} {
    font-size: {{ block.settings.eyebrow_size }}px;
    font-weight: 600;
    color: {{ block.settings.accent_color }};
    text-transform: uppercase;
    letter-spacing: 0.2em;
    margin-bottom: {{ block.settings.eyebrow_spacing }}px;
    display: block;
  }

  .ai-legacy-story__title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    font-weight: {{ block.settings.title_weight }};
    color: {{ block.settings.text_color }};
    line-height: 1.2;
    margin: 0 0 {{ block.settings.title_spacing }}px;
    letter-spacing: -0.02em;
  }

  .ai-legacy-story__subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    font-weight: 500;
    color: {{ block.settings.text_color }};
    opacity: 0.8;
    line-height: 1.4;
    margin: 0 0 {{ block.settings.subtitle_spacing }}px;
    font-style: italic;
  }

  .ai-legacy-story__description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_size }}px;
    color: {{ block.settings.text_color }};
    line-height: 1.8;
    opacity: 0.85;
    margin-bottom: {{ block.settings.description_spacing }}px;
  }

  .ai-legacy-story__description-{{ ai_gen_id }} p {
    margin: 0 0 1.5em;
  }

  .ai-legacy-story__description-{{ ai_gen_id }} p:last-child {
    margin-bottom: 0;
  }

  .ai-legacy-story__quote-{{ ai_gen_id }} {
    position: relative;
    padding: {{ block.settings.quote_padding }}px;
    margin: {{ block.settings.quote_spacing }}px 0;
    border-left: 4px solid {{ block.settings.accent_color }};
    background-color: {{ block.settings.quote_bg_color }};
    border-radius: {{ block.settings.quote_border_radius }}px;
  }

  .ai-legacy-story__quote-text-{{ ai_gen_id }} {
    font-size: {{ block.settings.quote_size }}px;
    font-style: italic;
    color: {{ block.settings.text_color }};
    line-height: 1.6;
    margin: 0;
  }

  .ai-legacy-story__quote-author-{{ ai_gen_id }} {
    font-size: {{ block.settings.quote_author_size }}px;
    font-weight: 600;
    color: {{ block.settings.accent_color }};
    margin-top: 16px;
    display: block;
  }

  .ai-legacy-story__cta-wrapper-{{ ai_gen_id }} {
    margin-top: {{ block.settings.cta_spacing }}px;
    display: flex;
    gap: 16px;
    {% if block.settings.text_align == 'center' %}
      justify-content: center;
    {% elsif block.settings.text_align == 'right' %}
      justify-content: flex-end;
    {% else %}
      justify-content: flex-start;
    {% endif %}
    flex-wrap: wrap;
  }

  .ai-legacy-story__cta-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    padding: {{ block.settings.button_padding_v }}px {{ block.settings.button_padding_h }}px;
    font-size: {{ block.settings.button_size }}px;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    transition: all 0.3s ease;
    border: 2px solid transparent;
  }

  .ai-legacy-story__cta-primary-{{ ai_gen_id }} {
    background-color: {{ block.settings.button_bg_color }};
    color: {{ block.settings.button_text_color }};
    border-color: {{ block.settings.button_bg_color }};
  }

  .ai-legacy-story__cta-primary-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_bg_color }};
    border-color: {{ block.settings.button_hover_bg_color }};
    color: {{ block.settings.button_hover_text_color }};
    transform: translateY(-2px);
  }

  .ai-legacy-story__cta-secondary-{{ ai_gen_id }} {
    background-color: transparent;
    color: {{ block.settings.text_color }};
    border-color: {{ block.settings.text_color }};
  }

  .ai-legacy-story__cta-secondary-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.text_color }};
    color: {{ block.settings.background_color }};
    transform: translateY(-2px);
  }

  .ai-legacy-story__divider-{{ ai_gen_id }} {
    width: {{ block.settings.divider_width }}px;
    height: {{ block.settings.divider_height }}px;
    background-color: {{ block.settings.accent_color }};
    margin: {{ block.settings.divider_spacing }}px {% if block.settings.text_align == 'center' %}auto{% elsif block.settings.text_align == 'right' %}0 0 0 auto{% else %}0{% endif %};
    border-radius: 2px;
  }

  @media screen and (max-width: 991px) {
    .ai-legacy-story__title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.75 }}px;
    }

    .ai-legacy-story__text-wrapper-{{ ai_gen_id }} {
      padding: {{ block.settings.text_padding | times: 0.6 }}px;
    }

    .ai-legacy-story-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding | times: 0.6 }}px 15px;
    }
  }

  @media screen and (max-width: 768px) {
    .ai-legacy-story__title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.6 }}px;
    }

    .ai-legacy-story__cta-wrapper-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: stretch;
    }

    .ai-legacy-story__cta-{{ ai_gen_id }} {
      width: 100%;
    }
  }
{% endstyle %}

<div class="ai-legacy-story-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-legacy-story__container-{{ ai_gen_id }}">
    <div class="ai-legacy-story__content-{{ ai_gen_id }}">
      {% if block.settings.image != blank or block.settings.show_image %}
        <div class="ai-legacy-story__image-wrapper-{{ ai_gen_id }}">
          <div class="ai-legacy-story__image-container-{{ ai_gen_id }}">
            {% if block.settings.image != blank %}
              <img
                src="{{ block.settings.image | image_url: width: 1200 }}"
                alt="{{ block.settings.image.alt | escape }}"
                loading="lazy"
                width="{{ block.settings.image.width }}"
                height="{{ block.settings.image.height }}"
                class="ai-legacy-story__image-{{ ai_gen_id }}"
              >
            {% else %}
              <div class="ai-legacy-story__image-placeholder-{{ ai_gen_id }}">
                {{ 'image' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>
        </div>
      {% endif %}

      <div class="ai-legacy-story__text-wrapper-{{ ai_gen_id }}">
        {% if block.settings.eyebrow != blank %}
          <span class="ai-legacy-story__eyebrow-{{ ai_gen_id }}">
            {{ block.settings.eyebrow }}
          </span>
        {% endif %}

        {% if block.settings.show_divider and block.settings.divider_position == 'top' %}
          <div class="ai-legacy-story__divider-{{ ai_gen_id }}"></div>
        {% endif %}

        {% if block.settings.title != blank %}
          <h2 class="ai-legacy-story__title-{{ ai_gen_id }}">
            {{ block.settings.title }}
          </h2>
        {% endif %}

        {% if block.settings.subtitle != blank %}
          <div class="ai-legacy-story__subtitle-{{ ai_gen_id }}">
            {{ block.settings.subtitle }}
          </div>
        {% endif %}

        {% if block.settings.show_divider and block.settings.divider_position == 'middle' %}
          <div class="ai-legacy-story__divider-{{ ai_gen_id }}"></div>
        {% endif %}

        {% if block.settings.description != blank %}
          <div class="ai-legacy-story__description-{{ ai_gen_id }}">
            {{ block.settings.description }}
          </div>
        {% endif %}

        {% if block.settings.show_quote and block.settings.quote_text != blank %}
          <div class="ai-legacy-story__quote-{{ ai_gen_id }}">
            <p class="ai-legacy-story__quote-text-{{ ai_gen_id }}">
              {{ block.settings.quote_text }}
            </p>
            {% if block.settings.quote_author != blank %}
              <span class="ai-legacy-story__quote-author-{{ ai_gen_id }}">
                — {{ block.settings.quote_author }}
              </span>
            {% endif %}
          </div>
        {% endif %}

        {% if block.settings.show_divider and block.settings.divider_position == 'bottom' %}
          <div class="ai-legacy-story__divider-{{ ai_gen_id }}"></div>
        {% endif %}

        {% if block.settings.button_text != blank or block.settings.button_text_secondary != blank %}
          <div class="ai-legacy-story__cta-wrapper-{{ ai_gen_id }}">
            {% if block.settings.button_text != blank %}
              <a
                href="{{ block.settings.button_link }}"
                class="ai-legacy-story__cta-{{ ai_gen_id }} ai-legacy-story__cta-primary-{{ ai_gen_id }}"
              >
                {{ block.settings.button_text }}
              </a>
            {% endif %}

            {% if block.settings.button_text_secondary != blank %}
              <a
                href="{{ block.settings.button_link_secondary }}"
                class="ai-legacy-story__cta-{{ ai_gen_id }} ai-legacy-story__cta-secondary-{{ ai_gen_id }}"
              >
                {{ block.settings.button_text_secondary }}
              </a>
            {% endif %}
          </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>

